{
  "name": "mutex-server",
  "version": "0.3.0",
  "description": "Mutex Server using WebSocket",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "api": "typedoc src ../tstl/src --exclude \"**/+(test|benchmark)/**\" --excludeNotDocumented --plugin typedoc-plugin-external-module-name --plugin typedoc-plugin-exclude-references --out ../mutex.dev/api",
    "build": "npm run compile",
    "compile": "rimraf lib && tsc",
    "dev": "rimraf lib && tsc --watch",
    "test": "node lib/test",
    "test:ts": "ts-node src/test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/samchon/mutex-server"
  },
  "keywords": [
    "mutex",
    "semaphore",
    "condition",
    "variable",
    "latch",
    "barrier",
    "write",
    "read",
    "unique",
    "shared",
    "lock",
    "acquire",
    "server",
    "remote",
    "critical",
    "section",
    "thread",
    "tstl",
    "tgrid"
  ],
  "author": {
    "name": "Jeongho Nam",
    "email": "samchon@samchon.org",
    "url": "http://samchon.org"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/samchon/mutex-server/issues"
  },
  "homepage": "https://mutex.dev",
  "devDependencies": {
    "@types/cli": "^0.11.19",
    "@types/node": "^13.11.1",
    "cli": "^1.0.1",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.16",
    "ts-node": "^8.8.2",
    "typedoc": "^0.22.10",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "tgrid": "^0.8.4",
    "tstl": "^2.5.0"
  }
}
